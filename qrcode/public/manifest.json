{
    "manifest_version": 3,
    "name": "QR code generator",
    "description": "Creates a QR code for the page you are using",
    "version": "1.0",
    "icons": {
      "16": "assets/images/qrcode-solid-16.png",
      "32": "assets/images/qrcode-solid-32.png",
      "48": "assets/images/qrcode-solid-48.png",
      "128": "assets/images/qrcode-solid-128.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "assets/images/qrcode-solid-16.png",
        "32": "assets/images/qrcode-solid-32.png",
        "48": "assets/images/qrcode-solid-48.png",
        "128": "assets/images/qrcode-solid-128.png"
      }
    },
    "permissions": ["scripting", "activeTab"],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
        }
      }
    }
  }